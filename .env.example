# 接口访问频率限制
RATE_LIMITS=60          # 周期内允许次数
RATE_LIMITS_EXPIRES=1   # 周期，单位分钟

# 发布内容接口频率限制（发帖与评论）
PUBLISH_RATE_LIMITS=10
PUBLISH_RATE_LIMITS_EXPIRES=1
